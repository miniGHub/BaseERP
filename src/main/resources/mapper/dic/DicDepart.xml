<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<!-- 设置为IUserDao接口方法提供sql语句配置 -->
<mapper namespace="com.mini.dao.dic.IDicDepart">

    <select id="SelectAll" resultType="DIC_DEPART">
        SELECT * FROM DIC_DEPART
    </select>

    <select id="SelectName" resultType="String" parameterType="int">
        SELECT * FROM DIC_DEPART WHERE depart_id = #{departId}
    </select>

</mapper>